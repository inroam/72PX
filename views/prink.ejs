<% include header  %>
<main id="prink">
    <!----------------摄影排行榜 开始----------------------->
    <header class="container-fluid">全部 > <%= prinks.count %>位化妆师</header>
    <article id="prinkMain" class="container-fluid items">
        <ul>
            <% prinks.data.forEach(function(prink, index){ %>
            <li class="row">
                <div class="prinker col-lg-4 col-sm-4">
                    <div class="row">
                        <a href="javascript:void(0)" class="avatar ashow col-lg-4 col-sm-4"><img src="<%= prink.cover %>" class="img-responsive" /> </a>
                        <div class="info col-lg-8 col-sm-8">
                            <h3><%= prink.name %></h3>
                            <p class="hits text-muted">风格：<%= prink.style %></p>
                            <p class="hits text-muted">认证：<%= prink.tags %></p>
                            <p class="text-muted">尺度：<%= prink.scale %></p>
                            <p class="text-muted">报酬：<%= prink.mark %></p>
                        </div>
                    </div>
                </div>
                <div class="ablum detail<%= index %> col-lg-8 col-sm-8">
                    <div class="row">
                        <% prink.imageList.forEach(function(image, index){ %>
                        <% if(index < 4){ %>
                        <a href="javascript:void(0)" class="show col-lg-3 col-sm-3"><img src="<%= image %>" class="img-responsive detailImg<%= index %>" /></a>
                        <% } %>
                        <% }) %>
                    </div>
                </div>
            </li>
            <% }) %>
        </ul>
    </article>
    <!----------------摄影排行榜 结束----------------------->
</main>
<% include footer %>
<script src="/js/prink.js"></script>